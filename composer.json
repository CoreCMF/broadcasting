{
    "name": "corecmf/broadcasting",
    "type": "Library",
    "description": "corecmf broadcasting package",
    "keywords": [
        "CoreCMF",
        "broadcasting",
        "alipay",
        "wechat"
    ],
    "homepage": "https://github.com/CoreCMF/broadcasting",
    "license": "laravel vue corecmf broadcasting",
    "authors": [
        {
            "name": "bigrocs",
            "email": "bigrocs@qq.com",
            "homepage": "http://www.bigrocs.vip",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "corecmf/admin": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "CoreCMF\\Broadcasting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CoreCMF\\Broadcasting\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
